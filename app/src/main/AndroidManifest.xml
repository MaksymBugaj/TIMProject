<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maksy.timproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login.LoginActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Login.CreateAccount"
            android:parentActivityName=".Login.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Login.AfterLogin.AfterLogin"
            android:parentActivityName=".Login.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Calendar.CalendarActivity"
            android:parentActivityName=".Login.AfterLogin.AfterLogin"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Calendar.CalendarChooseDate"
            android:parentActivityName=".Calendar.CalendarActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".API.APIExample" />
        <activity android:name=".API.APIRetrofit" />
        <activity android:name=".Appointments.CreateAppointment" />
        <activity android:name=".Appointments.PatientAppointments" />
        <activity android:name=".Treatments.AddTreatment" />
        <activity android:name=".Location.Location" />
        <activity android:name=".Calendar.CreateDoctorAppointment" />
        <activity android:name=".Appointments.AvailableAppointments" />
        <activity android:name=".Appointments.DoctorPatient.DoctorPatients"></activity>
    </application>

</manifest>